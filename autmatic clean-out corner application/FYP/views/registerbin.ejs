
    <%- include("./partials/head.ejs") %>

  <body id="page-top">
   
    <div id="wrapper">
 
      <%- include("./partials/sideBar.ejs") %>
      

      <div id="content-wrapper" class="d-flex flex-column">
        
        <div id="content">
      
              <%- include("./partials/topbar.ejs") %>
    <div class="col-lg-4 d-none d-lg-block"></div>
      
    </div>
    <div class="row mt-5">
      <div class="col-md-6 m-auto">
        <div class="card card-body">
          <h1 class="text-center mb-3">
            
          </h1>

         
          <form action="/users/registerlocality" method="POST">
            <div class="form-group">
              <label for="state">State</label>
              <input
                type="state"
                id="state"
                name="state"
                class="form-control"
                placeholder="Enter State"
                value="<%= typeof state != 'undefined' ? state : '' %>"
              />
            </div>
            <div class="form-group">
              <label for="areaPin">Pin Code</label>
              <input
                type="areaPin"
                id="areaPin"
                name="areaPin"
                class="form-control"
                placeholder="Enter Area Pincode"
                value="<%= typeof areaPin != 'undefined' ? areaPin : '' %>"
              />
            </div>
            <!--  !areaPin || !latt || !longg || !assignedCode -->
            <div class="form-group">
              <label for="latt">Enter Latitude</label>
              <input
                type="latt"
                id="latt"
                name="latt"
                class="form-control"
                placeholder="Enter Latitude"
                value="<%= typeof latt != 'undefined' ? latt : '' %>"
              />
            </div>
            <div class="form-group">
              <label for="longg">Enter Longitude</label>
              <input
                type="longg"
                id="longg"
                name="longg"
                class="form-control"
                placeholder="Enter Longitude"
                value="<%= typeof longg != 'undefined' ? longg : '' %>"
              />
            </div>
            <div class="form-group">
              <label for="assignedCode">Area Code</label>
              <input
                type="assignedCode"
                id="assignedCode"
                name="assignedCode"
                class="form-control"
                placeholder="Enter Area Code"
                value="<%= typeof password != 'undefined' ? password : '' %>"
              />
            </div>
            <button type="submit" class="btn btn-primary btn-block">
              Register Bin Information
            </button>
          </form>
        </div>
      </div>
    </div>
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <script src="vendor/jquery-easing/jquery.easing.min.js"></script>

    <script src="js/sb-admin-2.min.js"></script>
  
  </body>
  <%- include("./partials/footer.ejs") %>
</html>
